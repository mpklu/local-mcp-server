# Environment variables for Google Gemini CLI integration
# Copy this file to .env and update with your actual values

# Google AI Studio API Key
# Get this from https://aistudio.google.com/app/apikey
GOOGLE_AI_STUDIO_KEY=your-api-key-here

# Google Cloud Project ID
# The project that has Gemini API access enabled
GEMINI_PROJECT_ID=your-project-id

# Gemini CLI Configuration Directory
# Where Gemini CLI looks for configuration files
GEMINI_CLI_CONFIG=/path/to/gemini/cli/config

# Optional: Debug settings
GEMINI_DEBUG=false
PYTHONPATH=/absolute/path/to/local-mcp-server/server/src

# Optional: Customize server behavior
MCP_TIMEOUT=30
MCP_MAX_OUTPUT_LENGTH=10000